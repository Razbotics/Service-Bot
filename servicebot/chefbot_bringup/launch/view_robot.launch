<!--
  Used for visualising arlobot in action.  
  
  It requires minimal.launch and optionally 3dsensor.launch to already be up and running.
 -->
<launch>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find chefbot_bringup)/rviz/robot.rviz" />
  <include file="$(find chefbot_bringup)/launch/joy_teleop.launch" />
</launch>
